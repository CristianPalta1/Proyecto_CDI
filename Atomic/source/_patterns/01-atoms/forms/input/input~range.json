{
    "inputType" : "range",
    "inputId" : "range",
    "inputName": "range",
    "inputAdditionalAttr": [
        {
            "attribute": "min",
            "value": "0"
        },
        {
            "attribute": "max",
            "value": "100"
        }
    ],
    "inputJsEvents": [
        {
            "event" : "onChange",
